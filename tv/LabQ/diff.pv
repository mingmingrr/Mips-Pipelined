module diff
	( input [255:0] in
	, input [7:0] sel
	, output out1
	, output out2
	);

always @(*)
	if(sel == 255)
		out1 = in[0];
$for(i in range(1, 256))
	else if(sel == $(255 - i))
		out1 = in[$(i)];
$endfor
	else
		out1 = 0;

always @(*)
	case(sel)
$for(i in range(256))
		$(255 - i) : out2 = in[$(i)];
$endfor
		default : out2 = 0;
	endcase

endmodule

